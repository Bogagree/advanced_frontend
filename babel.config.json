{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": [
          "ru",
          "en"
        ],
        "keysAsDefaultValues": true
      }
    ]
  ]
}
